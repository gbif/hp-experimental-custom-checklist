    { "source": "/publisher/!(search)", "destination": "/publisher/_key_.html" },
    { "source": "/publisher/!(search)/**", "destination": "/publisher/_key_.html" },
    { "source": "/resource/!(search)", "destination": "/resource/_key_.html" },
    { "source": "/resource/!(search)/**", "destination": "/resource/_key_.html" },
    { "source": "/occurrence/!(search)", "destination": "/occurrence/_key_.html" },
